# OpenAI API key
OPENAI_API_KEY=sk-your-openai-api-key

# Google Cloud settings
# Option 1: Path to credentials file
GOOGLE_APPLICATION_CREDENTIALS=path/to/botanist-call-assistant-key.json

# Option 2: Use JSON credential content directly (for cloud deployments)
# Uncomment and use this option for Koyeb deployment
# GOOGLE_CREDENTIALS_JSON={"type":"service_account","project_id":"your-project-id",...}

# Port for WebSocket server
PORT=8080

# Log level
LOG_LEVEL=info 